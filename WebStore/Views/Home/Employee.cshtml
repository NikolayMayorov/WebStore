@model System.Collections.Generic.IEnumerable<WebStore.Models.Employee>

<div>
<h1>Employees</h1>
<table>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.FirstName
            </td>
            <td>
                @item.SurName
            </td>
            <td>
            @item.Patronymic
            <td>
                @item.Age
            </td>
            <td>
                <a href="~/Home/EmployeeData/@item.Id"> Details</a>
            </td>
        </tr>


    }
</table>
</div>